.upper
	background #fba190
	width 100%
	height 100%
	padding-top 240px

	&__line
		width 1396px
		border-top 1px solid #fff
		position absolute
		top 493px
		left 305px
		transform(rotate(135deg))

	&__logo
		text-align center
		width 862px
		background #f4c6bd
		border 13px solid #fff
		box-shadow 1px 21px 0px -12px #f4a684
		margin 0 auto
		padding 29px 60px 60px
		position relative

		&:after
			position absolute
			content ''
			width 60px
			height 60px
			background #fbbf9a url(../images/sprite.png) no-repeat 14px -178px
			border 14px solid #fff
			transform (rotate(-45deg))
			top -90px
			right 515px
			box-shadow -9px 9px 1px 1px rgba(244,148,130, 0.55)

		&:before
			position absolute
			content ''
			width 60px
			height 60px
			background #fbbf9a url(../images/sprite.png) no-repeat 12px -247px
			border 14px solid #fff
			transform (rotate(-45deg))
			top -90px
			right 279px
			box-shadow -9px 9px 1px 1px rgba(244,148,130, 0.55)


	&__plus
		position absolute
		display block
		width 14px
		height 14px
		top -51px
		background url("../images/sprite.png") no-repeat -17px -89px
		left 387px

	&__title
		font 30px $black-caps
		letter-spacing 5px
		color #fff

	&__title2
		font 20px $black-caps
		letter-spacing 1px
		color #fff
		line-height 62px

	&__text
		font 15px $black-caps
		letter-spacing 2px
		color #fff
		line-height 26px

	&__arrow
		display inline-block
		width 30px
		height 30px
		background #f19b7d
		border 5px solid #fddcc8
		border-radius 50%
		box-shadow 2px 2px 4px 0px rgba(0,0,0,0.1)
		cursor pointer
		position absolute
		bottom 5%
		left 50%

		&:after
			content:""
			display block
			width 5px
			height 5px
			position absolute
			border 3px solid #fddcc8
			border-left-color transparent
			border-bottom-color transparent
			border-radius 3px
			transform (rotate(135deg))
			top 4px
			left 5px

