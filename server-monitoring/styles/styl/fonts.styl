@font-face
	font-family 'fontello'
	src url('fonts/fontello.eot?50809833')
	src url('fonts/fontello.eot?50809833#iefix') format('embedded-opentype'),
	   url('fonts/fontello.woff?50809833') format('woff'),
	   url('fonts/fontello.ttf?50809833') format('truetype'),
	   url('fonts/fontello.svg?50809833#fontello') format('svg')
	font-weight normal
	font-style normal

[class^="icon-"]:before,
[class*=" icon-"]:before
	font-family 'fontello'
	font-style normal
	font-weight normal
	speak none

	display inline-block
	text-decoration inherit
	width 1em
	margin-right .2em
	text-align center
	/* opacity: .8; */

	/* For safety - reset parent styles, that can break glyph codes*/
	font-variant normal
	text-transform none

	/* fix buttons height, for twitter bootstrap */
	line-height 1em

	/* Animation center compensation - margins should be symmetric */
	/* remove if not needed */
	margin-left .2em

	/* Font smoothing. That was taken from TWBS */
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

.icon-chart-line:before
	content '\e801'
.icon-thumbs-down-alt:before
	content '\e803'
.icon-thumbs-up-alt:before 
	content '\e804'
.icon-download-cloud:before
	content '\e805'
.icon-gauge:before
	content '\e800'