@font-face
	font-family 'customfont'
	src url('fonts/customfont.eot?dqttk')
	src url('fonts/customfont.eot?dqttk#iefix') format('embedded-opentype'),
		url('fonts/customfont.ttf?dqttk') format('truetype'),
		url('fonts/customfont.woff?dqttk') format('woff'),
		url('fonts/customfont.svg?dqttk#customfont') format('svg')
	font-weight normal
	font-style normal

[class^="icon-"], [class*=" icon-"]
	font-family 'customfont'
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1

	/* Better Font Rendering =========== */
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

.icon-chart:before
	content "\e900"
.icon-statistic:before
	content "\e901"
.icon-upload-cloud:before
	content "\e902"
